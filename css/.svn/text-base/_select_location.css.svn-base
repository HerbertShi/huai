@charset "utf-8";
#sl_location_mask {
	background-color:#cccccc;
	position:absolute;
	-moz-opacity:0.65;
	filter:Alpha(Opacity=65);
	z-index:100;
}

#sl_location_hidden_div {
	background:#fff;
	z-index:100;
	position:absolute;
	border:solid 2px #869fc5;
	text-align:left;
	width:550px;
	padding:8px;
}
#sl_location_selector .operation_label{
	color:#006;
	margin-left:20px;
	letter-spacing:1px;
	width:60px;
	font-weight:normal;
	line-height:180%;
}
.box_10{
	clear:both;
	width:100%;
	height:10px;
}
#sl_location_select_title{
	width:100%;
	height:28px;
	margin-bottom:6px;
	margin-top:10px;
}
#sl_location_selector ul, #sl_list_selected_location ul, #sl_add_selected_location ul{
	list-style:none;
}
span.location_sel_title {
	text-indent:15px;
	color:#006;
	font-weight:bold;
	line-height:13px;
}
#sl_list_selected_location, #sl_add_selected_location{
	clear:both;
	margin-left:31px;
}
#sl_add_selected_location{
	margin-bottom:10px;
}
#sl_add_selected_location ul {
	list-style:none;
	width:380px;
	border-bottom:1px dashed #e1e1e1;
	margin-bottom:5px;
	padding-bottom:4px;
}
#sl_add_selected_location li{
	float:left;
	width:320px;
	margin-top:5px;
	padding-top:4px;
	padding-bottom:2px;
}
.location_select_tip{
	font-family:Tahoma, Arial;
	font-size:14px;
	color:#222;
	line-height:30px;
	text-indent:20px;
	display:block;
}